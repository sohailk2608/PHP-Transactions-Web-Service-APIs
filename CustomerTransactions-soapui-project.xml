nValues" id="21f69b8c-4586-4c4b-a7a7-c212d230428f"><con:settings/><con:config><script>//Here we are trying to Extract TransactionId from the response

import groovy.json.JsonSlurper

testStep = testRunner.testCase.testSteps["GETAllTransactions"]

//creating global variables that we will use later
def transArray = ""
def transId = ""
long nxtTrsId = 0

def response = testStep.getProperty("response").value; //This stores the response into a response variable

log.info response //simple echo to view the response

if (response == null) {
	log.info('No Response found...');
}else {
	def jSlurper = new JsonSlurper();
	def json = jSlurper.parseText(response); // Here we use JsonSlurper to parse response into processing

	transArray = json.transaction_id;
	
	for(i=0; i&lt;transArray.size;i++){// Here the Array size is taken to get the number of Transaction entries.
		transId = json.transaction_id[i];
		//We need to convert value of Transction ID from String to Int so that it can be incriminted by one
		nxtTrsId = Integer.parseInt(transId) + 1; //Transaction ID passed will be one more than the Latest TransactionID
	}
	
	String nxtTrsIdString = String.valueOf(nxtTrsId);// Converted from Int to String so that it can be passed to Custom Property
	
	testRunner.testCase.testSuite.project.setPropertyValue("trs_id", nxtTrsIdString)
	log.info "The next transaction id value is set as \""+ nxtTrsIdString + "\"."

	return "The next transaction id value is set as \""+ nxtTrsIdString + "\"."
}
</script></con:config></con:testStep><con:testStep type="restrequest" name="PUTAddTransctionsByID" id="b105a209-5d9a-45bf-b7d9-b6800b8f1fdf"><con:settings/><con:config service="http://transactions" resourcePath="/transactionservice/transaction/${#Project#trs_id}" methodName="PUTAddTransctionsByID" xsi:type="con:RestRequestStep" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"><con:restRequest name="PUTAddTransctionsByID" id="3fbf8b63-9257-4cc9-9459-eaf9fc428174" mediaType="application/json" postQueryString="false"><con:settings><con:setting id="com.eviware.soapui.impl.wsdl.WsdlRequest@request-headers">&lt;xml-fragment/></con:setting></con:settings><con:endpoint>http://transactions</con:endpoint><con:request>{
